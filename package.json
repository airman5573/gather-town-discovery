{
  "name": "gather-town-discovery",
  "version": "1.0.0",
  "description": "discovery for gather town",
  "main": "index.js",
  "scripts": {
    "install-dependencies": "npm install && cd api && npm install && cd ../client && npm install",
    "start:dev:api": "cd api && npm run start:dev",
    "start:dev:client": "cd client && npm run dev",
    "build": "cd api && npm run build && cd ../client && npm run build",
    "start:prod": "cd api && npm run start:prod && cd ../client && npm run start:prod",
    "restart": "pm2 delete all && npm run start:production",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/airman5573/gather-town-discovery.git"
  },
  "author": "airman5573",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/airman5573/gather-town-discovery/issues"
  },
  "homepage": "https://github.com/airman5573/gather-town-discovery#readme",
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "commitizen": "^4.2.4",
    "commitlint-config-gitmoji": "^2.2.5",
    "cz-customizable": "^6.3.0",
    "husky": "^7.0.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    }
  }
}
